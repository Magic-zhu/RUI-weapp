@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'), url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 24px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-editor-bold:before {
  content: "\e516";
}
.icon-editor-clean:before {
  content: "\e517";
}
.icon-editor-image:before {
  content: "\e518";
}
.icon-editor-list-numbers:before {
  content: "\e51a";
}
.icon-editor-italic:before {
  content: "\e519";
}
.icon-editor-redo:before {
  content: "\e51b";
}
.icon-editor-undo:before {
  content: "\e51c";
}
.icon-editor-list-bulleted:before {
  content: "\e51e";
}
.rich-editor .toolbar-left {
  width: 80rpx;
  position: fixed;
  box-sizing: border-box;
  z-index: 5;
  top: 350rpx;
  right: 0rpx;
  background-color: rgba(255, 255, 255, 0.8);
}
.rich-editor .toolbar-top {
  width: 100%;
  height: 80rpx;
  box-sizing: border-box;
  display: flex;
  position: relative;
}
.rich-editor .toolbar-top .color-picker-top {
  width: 750rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  opacity: 1;
  z-index: 10;
}
.rich-editor .toolbar-top .color-picker-top .color-item {
  width: 60rpx;
  height: 60rpx;
  margin-left: 20rpx;
}
.rich-editor .toolbar-item {
  width: 80rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: grey;
}
.rich-editor .toolbar-item-hover {
  background-color: lightgrey;
}
.rich-editor .toolbar-item-select {
  background-color: lightgrey;
}
.rich-editor .red {
  color: red;
}
.rich-editor .highlight {
  width: 40rpx;
  height: 40rpx;
  background-color: yellow;
  display: flex;
  align-items: center;
  justify-content: center;
}
.rich-editor .second {
  right: 160rpx;
  font-size: 36rpx;
  font-weight: bold;
}
.rich-editor .second .underline {
  text-decoration: underline;
}
.rich-editor .color-picker-left {
  position: fixed;
  top: 350rpx;
  right: 0rpx;
  width: 80rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  opacity: 0;
}
.rich-editor .color-picker-left .color-item {
  width: 60rpx;
  height: 60rpx;
  margin-top: 40rpx;
}
.rich-editor .color-picker-left .moreColor {
  margin-top: 40rpx;
  font-size: 24rpx;
}
.rich-editor .color-circle {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 300px;
  height: 300px;
  z-index: 5;
}
.rich-editor .editor-placeholder {
  text-align: center;
}
